---
"extends":
  - "formidable/configurations/es6-react"

"rules":
  "indent": [2, 2, {"SwitchCase": 1}]
  "max-len": 0
  "no-magic-numbers": 0
  "react/prefer-es6-class": 0
  "react/no-multi-comp": 0
  "quotes": ["error", "single"]
  "arrow-parens": 0
  "arrow-spacing": 0
  "brace-style": 0
  "comma-dangle": 0
  "comma-spacing": 0
  "comma-style": 0
  "eol-last": 0
  "generator-star-spacing": 0
  "indent": 0
  "jsx-quotes": 0
  "key-spacing": 0
  "keyword-spacing": 0
  "new-parens": 0
  "no-extra-semi": 0
  "no-mixed-spaces-and-tabs": 0
  "no-multi-spaces": 0
  "no-multiple-empty-lines": 0
  "no-spaced-func": 0
  "no-trailing-spaces": 0
  "object-curly-spacing": 0
  "react/jsx-closing-bracket-location": 0
  "react/jsx-indent-props": 0
  "react/jsx-wrap-multilines": 0
  "semi": 0
  "semi-spacing": 0
  "space-before-blocks": 0
  "space-before-function-paren": 0
  "space-in-parens": 0
  "space-infix-ops": 0
  "space-unary-ops": 0

"env":
  "browser": true,
  "node": true

"globals":
  "afterEach": true,
  "describe": true,
  "expect": true,
  "it": true,
  "jest": true,
  "test": true
